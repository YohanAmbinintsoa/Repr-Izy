{
    "postgresql":{
        "connection_string":"jdbc:postgresql://{host}:5432/{database}",
        "driver":"org.postgresql.Driver"
    },
    "mysql":{
        "connection_string":"jdbc:mysql://{host}:3306/{database}",
        "driver":"com.mysql.cj.jdbc.Driver"
    },
    "oracle":{
        "connection_string":"jdbc:oracle:thin:@{host}:1521/{database}",
        "driver":"oracle.jdbc.OracleDriver"
    },
    "sqlserver":{
        "connection_string":"jdbc:sqlserver://{host}:1433;databaseName={database}",
        "driver":"com.microsoft.sqlserver.jdbc.SQLServerDriver"
    }
}
